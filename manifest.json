{
    "name": "Hello Extensions",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "hello.html"
    },
    "background": {
        "service_worker": "background.js"
      },
      "permissions": ["tabs"],
      "web_accessible_resources" : [{
        "resources":["Checklist_implementation.html"],
        "matches":["<all_urls>"]
      }]
  }